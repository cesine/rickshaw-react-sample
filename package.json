{
  "name": "rickshaw-react-sample",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:vanilla && npm run build:react",
    "build:react": "react-scripts build",
    "build:vanilla": "browserify charts.js -o dist.js",
    "lint": "eslint index.js chart.js test --fix",
    "start": "npm run start:vanilla && npm run start:react",
    "start:react": "react-scripts start",
    "start:vanilla": "node index.js",
    "test": "npm run test:vanilla && npm run test:react",
    "test:react": "CI=true react-scripts test --env=jsdom",
    "test:react:watch": "react-scripts test --env=jsdom",
    "test:vanilla": "mocha test",
    "eject": "react-scripts eject"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.13",
    "rickshaw": "^1.6.3"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "chai": "^4.1.2",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "fixturefiles": "^0.3.0",
    "mocha": "^3.5.1"
  }
}
